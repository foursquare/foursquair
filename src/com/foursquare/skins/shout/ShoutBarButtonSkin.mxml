<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	<s:Group left="0" right="0" top="0" bottom="0" buttonMode="true" useHandCursor="true">
		<s:Rect left="0" right="0" top="0" bottom="0">
			<s:fill>
				<s:SolidColor color="0xffffff" alpha="0" />
			</s:fill>
		</s:Rect> 
		<s:BitmapImage source="@Embed('assets/icons/icon-shout.png')"
				   top="5" left="5" />
		<s:BitmapImage source="@Embed('assets/icons/dimple-shoutbox.png')"
				   top="7" horizontalCenter="0"/>
	</s:Group>
	
</s:Skin>
